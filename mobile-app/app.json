{
  "expo": {
    "name": "M-dawa Patient",
    "slug": "m-dawa-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mdawa.mobile"
    },
    "android": {
      "package": "com.mdawa.mobile"
    },
    "plugins": [
      [
        "expo-secure-store"
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#667eea",
          "sounds": []
        }
      ]
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#667eea"
    }
  }
}
